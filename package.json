{
  "name": "hacker-news-clone",
  "version": "1.0.0",
  "description": "Hacker News clone site",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import"
