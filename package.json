    "html-webpack-plugin": "^5.3.1",
    "sass": "^1.32.12",
    "style-loader": "^2.0.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
